@model Timetable.Models.TimetableU
@{
    ViewBag.Title = "Удаление преподавателя";
}

<div>
    <dl class="dl-horizontal">
        <dt>Дата поставленной пары</dt>
        <dd>
            @Html.DisplayFor(model => model.Date)
        </dd>

        <dt>Id предмета</dt>
        <dd>
            @Html.DisplayFor(model => model.Id_Lesson)
        </dd>

        <dt>Id аудитории</dt>
        <dd>
            @Html.DisplayFor(model => model.Id_Class)
        </dd>

        <dt>Id преподавателя</dt>
        <dd>
            @Html.DisplayFor(model => model.Id_Teacher)
        </dd>
        <dt>Id пользователя</dt>
        <dd>
            @Html.DisplayFor(model => model.Id_User)
        </dd>
        <dt>Id группы</dt>
        <dd>
            @Html.DisplayFor(model => model.Id_Group)
        </dd>
    </dl>

    <div>
        <form asp-controller="Timetable" asp-action="DeleteTimetable" method="post">
            <div class="form-group">
                <input type="submit" class="btn btn-default" value="Удалить" />
            </div>
        </form>
    </div>
</div>