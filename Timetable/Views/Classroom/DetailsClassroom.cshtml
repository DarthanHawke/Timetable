@model Timetable.Models.Classroom
@{
    ViewBag.Title = Model.NumberClass;
}
<h2>Номер аудитории @Model.NumberClass</h2>
<div>
    <dl class="dl-horizontal">
        <dt>Индентификатор аудитории</dt>
        <dd>@Model.Id_Class</dd>

        <dt>Тип аудитории</dt>
        <dd>@Model.TypeClass</dd>

        <dt>Вместимость аудитории</dt>
        <dd>@Model.Capacity</dd>

        <dt>Наличие проектора</dt>
        <dd>@Model.Projector</dd>

        <dt>Наличие доски</dt>
        <dd>@Model.Board</dd>
    </dl>
</div>