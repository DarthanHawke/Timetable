@model Timetable.Models.Teacher
@{
    ViewBag.Title = "Добавление пользователя";
}
<form asp-action="create" asp-controller="home">
    <div class="form-group">
        <label asp-for="FIO" class="control-label">ФИО</label>
        <input type="text" asp-for="FIO" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="Specialization" class="control-label">Специальность</label>
        <input type="text" asp-for="Specialization" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="Department" class="control-label">Кафедра</label>
        <input type="text" asp-for="Department" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="Title" class="control-label">Звание</label>
        <input type="text" asp-for="Title" class="form-control" />
    </div>
    <div class="form-group">
        <input type="submit" value="Отправить" class="btn btn-default" />
    </div>
</form>